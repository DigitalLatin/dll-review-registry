<div class="container">
  <h1>Verify</h1>
  <form class="form-horizontal " action="/verify_result" method="get">
    <div class="form-group">
      <label class="control-label" style="width:100%;">Certificate URL</label>
      <input class="form-control" type="text" name="certificate_url"></input>
    </div>
    <div class="form-group">
      <label class="control-label" style="width:100%;">Signature Url</label>
      <textarea class="form-control" name="signature_url"></textarea>
    </div>
    <div class="form-group">
      <label class="control-label" style="width:100%;">Public Key Url</label>
      <textarea class="form-control" name="public_key_url"></textarea>
    </div>
    <div class="form-group">
      <button type="submit" class="btn btn-primary">Submit</button>
    </div>
  </form>
</div>
